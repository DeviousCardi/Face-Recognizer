   "source": [
    "img = cv.imread('road_img.jpeg')\n",
    "\n",
    "low   = np.array([10,10,10]) \n",
    "upper = np.array([120,120,120])\n",
    "\n",
    "img = cv.GaussianBlur(img, (5,5),0)\n",
    "\n",
    "mask = cv.inRange(img , low, upper)\n",
    "\n",
    "cv.imwrite('Mask.jpg',mask)"
